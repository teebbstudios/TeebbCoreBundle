<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>

        <service id="teebb.core.translation.annotation_translation" class="Teebb\CoreBundle\Translation\AnnotationTranslation"
                 public="true">
            <argument type="service" id="translator"/>
        </service>

        <service id="teebb.core.metadata.entity_type_metadata_factory"
                 class="Teebb\CoreBundle\Metadata\EntityTypeMetadataFactory" public="true">
            <argument type="service" id="teebb.core.translation.annotation_translation"/>
        </service>

    </services>
</container>
