<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"
>
    <services>
        <service id="teebb.core.controller.abstract_entity_type_controller"
                 class="Teebb\CoreBundle\Controller\Types\AbstractEntityTypeController"
                 public="false">
            <argument type="service" id="teebb.core.template.registry"/>
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
            <argument type="service" id="debug.event_dispatcher"/>
            <argument type="service" id="teebb.core.form.contractor"/>
            <tag name="controller.service_arguments"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
            <call method="configure"/>
        </service>
        <service id="Teebb\CoreBundle\Controller\Types\AbstractEntityTypeController"
                 alias="teebb.core.controller.abstract_entity_type_controller" public="true"/>


        <service id="teebb.core.controller.abstract_content_controller"
                 class="Teebb\CoreBundle\Controller\AbstractContentController"
                 public="false">
            <tag name="controller.service_arguments"/>
            <argument type="service" id="doctrine.orm.default_entity_manager"/>
            <argument type="service" id="teebb.core.template.registry"/>
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>
        <service id="Teebb\CoreBundle\Controller\AbstractContentController"
                 alias="teebb.core.controller.abstract_content_controller" public="true"/>

    </services>
</container>
