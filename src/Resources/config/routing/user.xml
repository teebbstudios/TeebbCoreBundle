<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="teebb_user_substances_api" path="/user/autocomplete">
        <default key="_controller">Teebb\CoreBundle\Controller\Content\UserController::getSubstancesApi</default>
        <default key="route">teebb_user_substances_api</default>
        <default key="entity_class">Teebb\CoreBundle\Entity\User</default>
        <default key="query_label">email</default>
    </route>

</routes>
